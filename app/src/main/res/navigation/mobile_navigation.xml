<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_map">

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.jianan.parkwhere.ui.map.MapFragment"
        android:label="@string/title_map_view"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/navigation_list"
        android:name="com.jianan.parkwhere.ui.list.ListFragment"
        android:label="@string/title_list_view"
        tools:layout="@layout/fragment_list" />

    <fragment
        android:id="@+id/navigation_bookmarks"
        android:name="com.jianan.parkwhere.ui.bookmarks.BookmarksFragment"
        android:label="@string/title_bookmarks"
        tools:layout="@layout/fragment_list" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.jianan.parkwhere.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />

</navigation>